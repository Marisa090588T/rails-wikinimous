<% content_for(:title) do %>
  Wikinimous - <%= @article.title %>
<% end %>

<article>
  <h1><%= @article.title %></h1>
    <%= @article.content %>
    <hr>
      <%= link_to 'Edit', edit_article_path(@article) %> |
      <%= link_to 'Delete', article_path(@article), method: :delete, data: { confirm: 'Are you sure?' } %> |
      <%= link_to 'Back', articles_path %>
</article>
